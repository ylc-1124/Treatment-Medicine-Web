<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.sust.patient.mapper.PatientMapper">

    <select id="selectPatIdByUserId" resultType="java.lang.Integer">
        SELECT id FROM x_patient p WHERE p.user_id = #{userId}
    </select>
</mapper>
